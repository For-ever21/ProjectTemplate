    "lint-fix": "eslint --fix --ext .js,.vue,.ts ./",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
