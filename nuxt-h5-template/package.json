    "husky": "^4.3.7",
    "jest": "^26.5.0",
    "less": "^4.1.0",
    "less-loader": "5.0.0",
    "lint-staged": "^10.5.3",
    "postcss-pxtorem": "^5.1.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "svg-sprite-loader": "^5.2.1",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4",
    "vue-styleguidist": "^4.35.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
