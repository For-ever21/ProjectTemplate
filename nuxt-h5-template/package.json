{
  "name": "c-platform-h5-nuxt",
  "version": "2.7.0",
  "description": "新浪健康，严肃医疗资讯平台，健康无忧",
  "private": true,
  "scripts": {
    "serve": "cross-env ENV_NAME=production NODE_ENV=development --max-old-space-size=4096 nuxt",
    "serve:dev": "cross-env ENV_NAME=dev NODE_ENV=development --max-old-space-size=4096 nuxt",
    "serve:test": "cross-env ENV_NAME=test NODE_ENV=development --max-old-space-size=4096 nuxt",
    "serve:rebuild-dev": "cross-env ENV_NAME=rebuild-dev NODE_ENV=development --max-old-space-size=4096 nuxt",
    "serve:rebuild-test": "cross-env ENV_NAME=rebuild-test NODE_ENV=development --max-old-space-size=4096 nuxt",
    "serve:uat": "cross-env ENV_NAME=uat NODE_ENV=development --max-old-space-size=4096 nuxt",
    "build": "cross-env ENV_NAME=production NODE_ENV=production nuxt build",
    "build:dev": "cross-env ENV_NAME=dev NODE_ENV=production nuxt build",
    "build:test": "cross-env ENV_NAME=test NODE_ENV=production nuxt build",
    "build:rebuild-dev": "cross-env ENV_NAME=rebuild-dev NODE_ENV=production nuxt build",
    "build:rebuild-test": "cross-env ENV_NAME=rebuild-test NODE_ENV=production nuxt build",
    "build:uat": "cross-env ENV_NAME=uat NODE_ENV=production nuxt build",
    "release": "npx cross-env ENV_NAME=production NODE_ENV=production HOST='0.0.0.0' PORT=56005 --max-old-space-size=8196 node_modules/nuxt/bin/nuxt.js start ",
    "release:dev": "npx cross-env ENV_NAME=dev NODE_ENV=production HOST='0.0.0.0' PORT=56005 --max-old-space-size=8196 node_modules/nuxt/bin/nuxt.js start ",
    "release:test": "npx cross-env ENV_NAME=test NODE_ENV=production HOST='0.0.0.0' PORT=56005 --max-old-space-size=8196 node_modules/nuxt/bin/nuxt.js start ",
    "release:rebuild-dev": "npx cross-env ENV_NAME=rebuild-dev NODE_ENV=production HOST='0.0.0.0' PORT=56005 --max-old-space-size=8196 node_modules/nuxt/bin/nuxt.js start ",
    "release:rebuild-test": "npx cross-env ENV_NAME=rebuild-test NODE_ENV=production HOST='0.0.0.0' PORT=56005 --max-old-space-size=8196 node_modules/nuxt/bin/nuxt.js start ",
    "release:uat": "npx cross-env ENV_NAME=uat NODE_ENV=production HOST='0.0.0.0' PORT=56005 --max-old-space-size=8196 node_modules/nuxt/bin/nuxt.js start ",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lint-fix:js": "eslint --fix --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "lint-fix": "npm run lint-fix:js",
    "test": "jest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "dependencies": {
